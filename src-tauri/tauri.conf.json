{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "tauri-vanilla-sqlite",
  "version": "0.1.0",
  "identifier": "org.cacerreg.1",
  "build": {
    "frontendDist": "../dist",
    "beforeDevCommand": "npm run build",
    "beforeBuildCommand": "npm run build"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "tauri-vanilla-sqlite",
        "width": 800,
        "height": 600,
        "minWidth": 400,
        "minHeight": 300
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}